##### ea - alias for editing aliases
alias ea='sudo vim ~/.bash_aliases; source ~/.bash_aliases && source $HOME/.bash_aliases && echo "aliases sourced  --ok."'
alias ev='sudo vim /usr/share/vim/vimrc'

alias ga='git add .'
alias gs='git status'
alias gc='git commit -a -m'
alias gp='git push'
alias cdCPP='cd ~/GitHub/Learn_CPP/'
alias cdCUR='cd ~/GitHub/Learn_CPP/Chapter_7/Lectures/Stack_and_Heap'
alias cdALGO='cd ~/GitHub/Algorithms/Lab_2'
